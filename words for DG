public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    int t = 1000;
    String output = "{";

    for(int i = 0; i < 1000; i++){
        output = output + in.next() + ", ";
    }
    output = output + "}";
    System.out.print(output);
}

